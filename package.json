{
  "name": "@myku0814/scaffold-config-myku",
  "version": "0.0.9",
  "private": false,
  "type": "module",
  "description": "Scaffold Express Server develop by myku",
  "keywords": [
    "myku",
    "TypeScript",
    "Express",
    "MongoDB",
    "Scaffold"
  ],
  "author": "myku",
  "license": "proprietary",
  "engines": {
    "node": ">=20"
  },
  "main": "./src/index.js",
  "bin": {
    "create-scaffold-config-myku": "./bin/create.js"
  },
  "publishConfig": {
    "registry":"https://registry.npmjs.org"
  },
  "scripts": {
    "start": "npm run server:dev",
    "play": "cross-env NODE_ENV=\"development\" nodemon --watch \"src/**\" --ext \"ts,json\" --ignore \"src/**/*.spec.ts\" --exec \"node --import tsx ./src/dev/index.ts\"",
    "server:dev": "cross-env NODE_ENV=\"development\" nodemon --watch \"src/**\" --ext \"ts,json\" --ignore \"src/**/*.spec.ts\" --exec \"node --import tsx ./src/index.ts\"",
    "server:prod": "cross-env NODE_ENV=\"production\" node ./dist/src/index.js",
    "build": "run-s build:tsc",
    "build:tsc": "tsc --project ./tsconfig.json",
    "build:verbose": "tsc --project ./tsconfig.json --listFiles",
    "build:config": "tsc --project ./tsconfig.json --showConfig > ./dist/.myku/ts-config.txt",
    "build:diag": "tsc --project ./tsconfig.json --diagnostics > ./dist/.myku/ts-diagnostics.txt",
    "lint:verbose": "eslint . --debug --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix --ignore-path .gitignore",
    "lint:check": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --ignore-path .gitignore",
    "lint:fix": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix --ignore-path .gitignore",
    "format:check": "prettier --check src/ --ignore-path .gitignore",
    "format:fix": "prettier --write src/ --ignore-path .gitignore"
  },
  "dependencies": {
    "@myku0814/public-config-myku": "*",
    "ansi-colors": "^4.1.3",
    "busboy": "^1.6.0",
    "cors": "^2.8.5",
    "degit": "^2.8.4",
    "express": "^4.19.2",
    "inquirer": "^12.9.0",
    "jsonwebtoken": "^9.0.2",
    "mongoose": "^8.2.2"
  },
  "devDependencies": {
    "@types/busboy": "^1.5.3",
    "@types/cors": "^2.8.17",
    "@types/degit": "^2.8.6",
    "@types/express": "^4.17.21",
    "@types/jsonwebtoken": "^9.0.6",
    "@types/node": "^20.12.5",
    "cross-env": "^7.0.3",
    "eslint": "^8.57.0",
    "nodemon": "^3.1.0",
    "npm-run-all2": "^6.1.2",
    "prettier": "^3.2.5",
    "tsx": "^4.7.1",
    "typescript": "~5.4.0"
  }
}
